{
  "minzoom": 6,
  "maxzoom": 14,
  "name": "cyclenetworks",
  "id": "cyclenetworks",
  "basename": "cyclenetworks",
  "description": "Contains all the cyclenetworks of Belgium + a layer",
  "layers": [
    {
      "id": "cyclenetwork-genk",
      "description": "The routes in the Genk network",
      "type": "way",
      "filter": {
        "and": [
          {
            "key": "cyclenetwork"
          },
          {
            "key": "operator",
            "value": "Stad Genk"
          }
        ]
      },
      "split-tags":[
        "ref",
        "cyclecolour",
        "name"
      ]
    },
    {
      "id": "cyclenetwork",
      "description": "The actual edges in the network",
      "type": "way",
      
      "filter": {
        "and": [
          {
            "key": "cyclenetwork"
          },
          {
          "not": {
              "key": "state",
              "value": "proposed"
            }
          }
        ]
      }
    },
    {
      "id": "cyclenodes",
      "description": "The central cycling nodes",
      "type": "node",
      "filter": {
        "key": "rcn_ref",
      },
      "split-tags":[
        "ref",
        "cyclecolour",
        "name"
      ]
    }
  ]
}
